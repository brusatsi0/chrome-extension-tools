{
  "manifest_version": 2,
  "name": "web-accessible-resources",
  "description": "Test dynamic imports for content scripts",
  "version": "1.0.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": {
      "16": "images/icon-on-16.png"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ]
}
