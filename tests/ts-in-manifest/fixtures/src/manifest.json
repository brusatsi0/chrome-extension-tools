{
  "browser_action": {
    "default_icon": {
      "16": "images/icon-on-16.png"
    }
  },
  "background": {
    "scripts": ["background.ts"]
  },
  "permissions": [],

  "icons": {
    "128": "images/icon-on-128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.ts"]
    }
  ],

  "web_accessible_resources": [
    "chunks/*",
    "assets/*",
    "*.js.map"
  ]
}
