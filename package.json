    "magic-string": "^0.25.7",
    "mem": "^6.0.1",
    "read-pkg-up": "^8.0.0",
    "rxjs": "^7.3.0",
    "slash": "^3.0.0",
    "type-fest": "^2.2.0",
    "webextension-polyfill": "^0.6.0",
    "xstate": "^4.25.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
