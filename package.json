{
  "name": "rollup-plugin-chrome-extension",
  "version": "0.6.0",
  "description": "Build Chrome Extensions with this composed Rollup plugin.",
  "license": "MIT",
  "author": "Jack and Amy Steam <jacksteamdev@gmail.com>",
  "files": [
    "build"
  ],
  "repository": "bumble-org/rollup-plugin-chrome-extension",
  "main": "build/bundle-cjs.js",
  "module": "build/bundle-esm.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest",
    "preversion": "npm test && npm run build",
    "postversion": "git push && npm publish"
  },
  "jest": {
    "clearMocks": true,
    "modulePathIgnorePatterns": [
      "fixtures/dest"
    ],
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ],
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "dependencies": {
    "@bumble/manifest": "^0.3.2",
    "cheerio": "^1.0.0-rc.2",
    "fs-extra": "^7.0.1",
    "magic-string": "^0.25.2",
    "rollup-plugin-zip": "^0.3.0"
  },
  "devDependencies": {
    "@bumble/chrome": "^0.1.1",
    "@bumble/clipboard": "^0.2.2",
    "@bumble/stream": "^0.2.2",
    "@sucrase/jest-plugin": "^2.0.0",
    "chrome-promise": "^3.0.4",
    "eslint": "5.15.1",
    "jest": "^24.5.0",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^3.4.4",
    "rollup": "^1.7.3",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-pluginutils": "^2.5.0",
    "rxjs": "^6.4.0",
    "simple-git": "^1.110.0",
    "sinon": "^7.3.1",
    "sucrase": "^3.9.5",
    "unique-selector": "^0.4.1"
  },
  "peerDependencies": {
    "rollup": "^1.6.0"
  }
}
